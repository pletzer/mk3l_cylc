[scheduler]
    allow implicit tasks = False
[scheduling]
    cycling mode = integer
    [[graph]]
        R1 = """
            init => model?
            model:fail? => restore
        """
        P1 = """
            model[-P1]:succeed? => model?
            model:fail? => restore
        """
[runtime]
    [[init]]
    [[setup]]
    [[model]]
    [[isFinished]]
    [[save]]
    [[restore]]
    [[end]]
