[scheduler]
    allow implicit tasks = False
[scheduling]
    cycling mode = integer
    [[graph]]
        R1 = """
            model => isFinished?
        """
        P1 = """
            isFinished[-P1]:fail? => model => isFinished?
            isFinished:succeed? => end
        """
[runtime]
    [[init]]
    [[setup]]
    [[model]]
    [[isFinished]]
    [[save]]
    [[restore]]
    [[end]]
